  <div id="btnDis"
    class="maint bg-[#7db7ce] hover:bg-[#5A8394] transform transition-transform hover:scale-105">
    <img class="w-[200px] h-[200px] ml-[120px] mt-3"
      src="images/dash-icons/truck_icon.png" ondragstart="return false;">
